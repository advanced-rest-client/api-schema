          required: true
        Depreciation:
          type: number
          required: true
        Age:
          type: number
          required: true
        AssetDescription:
          type: string
          required: true
        CabType:
          type: string
          required: true
        Color:
          type: string
          required: true
        TCUType:
          type: string
          required: true
        TCUHours:
          type: number
          required: true
