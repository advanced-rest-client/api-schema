          type: string
          required: true
        Type:
          type: string
          required: true
        Weight:
          type: number
          required: true
        Serial:
          type: string
          required: true
        AcquiredDate:
          type: date-only
          required: true
        InService:
          type: date-only
          required: true
        MaintenanceCode:
          type: string
          required: true
        MaintenanceCodeDescription:
          type: string
          required: true
        AS400ECM:
          type: number
          required: true
        AS400Status:
          type: string
          required: true
        IQDate:
          type: date-only
          required: true
        Engine:
          type: string
          required: true
